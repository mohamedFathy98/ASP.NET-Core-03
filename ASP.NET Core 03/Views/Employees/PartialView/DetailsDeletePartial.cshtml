@model EmployeeViewModel
<div class="ms-2 me-2">
    <div class="mt-2 ms-2 me-2">
        <label asp-for="Name" class="form-label"></label>
        <input readonly asp-for="Name" class="form-control " placeholder="Name">

    </div>
    <div class="mt-2 ms-2 me-2">
        <label asp-for="Age" class="form-label"></label>
        <input readonly asp-for="Age" class="form-control " placeholder="Age">

    </div>
    <div class="mt-2 ms-2 me-2">
        <label asp-for="Address" class="form-label"></label>
        <input readonly asp-for="Address" class="form-control " placeholder="Address">

    </div>
    <div class="mt-2 ms-2 me-2">
        <label asp-for="Salary" class="form-label"></label>
        <input readonly asp-for="Salary" class="form-control " placeholder="Salary">

    </div>
    <div class="mt-2 ms-2 me-2">
        <label asp-for="Email" class="form-label"></label>
        <input readonly asp-for="Email" class="form-control " placeholder="Email">

    </div>
    <div class="mt-2 ms-2 me-2">
        <label asp-for="Phone" class="form-label"></label>
        <input readonly asp-for="Phone" class="form-control " placeholder="Phone">

    </div>
    <div class="mt-2 ms-2 me-2">
        <label asp-for="IsActive" class="form-label"></label>
        <input disabled asp-for="IsActive">

    </div>
