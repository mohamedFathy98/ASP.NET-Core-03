@model RoleViewModel
@{
    ViewData["Title"] = "Create";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<h1 class="mt-2">Create New Role</h1>
<form class="mt-3 shadow" asp-action="Create" enctype="multipart/form-data">

    <partial name="PartialView/CreatEditPartial"></partial>
    <div class="mt-2 ms-2">
        <button type="submit" class="btn btn-primary">Save</button>
        <a class="btn btn-danger ms-2" asp-action="Index">Cancel</a>
    </div>
</form>
@section Validation {
    <script src="~/lib/jquery-validation/dist/jquery.validate.min.js"></script>
    <script src="~/lib/jquery-validation-unobtrusive/jquery.validate.unobtrusive.min.js"></script>
}
